    "test": "ava"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alextes/url-sub.git"
  },
  "keywords": [
    "url",
    "formatting",
    "querystring",
    "util"
  ],
  "author": "Alexander Tesfamichael <alex.tesfamichael@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alextes/url-sub/issues"
  },
  "homepage": "https://github.com/alextes/url-sub#readme",
  "devDependencies": {
    "ava": "^4.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "query-string": "^7.1.1"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "tests/**/*"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
